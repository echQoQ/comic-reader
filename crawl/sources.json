{
	"biqu": {
		"baseUrl": "https://www.biqumh.com/",
		"search": {
			"url": "index.php/search?key={keyword}",
			"comic_item_selector": ".common-comic-item",
			"title_selector": ".comic__title > a",
			"url_selector": "div > a",
			"cover_img": ".cover > img",
			"cover_img_attribute": "data-original"
		},
		"chapters": {
			"chapter_item_selector": ".chapter__item",
			"chapter_title_selector": ".j-chapter-link",
            "chapter_url_selector": ".j-chapter-link",
			"href_selector": "href"
		},
		"images": {
			"selector": ".read-container > .rd-article-wr > .rd-article__pic > img",
			"src_attribute": "data-original"
		}
	},
	"manhuaol": {
		"baseUrl": "https://www.manhuaol.com/",
		"search": {
			"url": "/search?key={keyword}",
			"comic_item_selector": ".vbox",
			"title_selector": "h4 > a",
			"url_selector": "h4 > a",
			"cover_img": ".vbox_t > mip-img",
			"cover_img_attribute": "src"
		},
		"chapters": {
			"chapter_item_selector": ".list_block > li",
			"chapter_title_selector": "a",
            "chapter_url_selector": "a",
			"href_selector": "href"
		},
		"images": {
			"selector": "img",
			"src_attribute": "src"
		}
	},
	"baozimh" : {
		"baseUrl": "https://www.baozimh.com/",
		"search": {
			"url": "/search?q={keyword}",
			"comic_item_selector": ".comics-card",
			"title_selector": ".comics-card__title > h3",
			"url_selector": ".comics-card__info",
			"cover_img": "div a.comics-card__poster.text-decoration-none > amp-img",
			"cover_img_attribute": "src"
		},
		"chapters": {
			"chapter_item_selector": ".comics-chapters",
			"chapter_title_selector": "a > div > span",
            "chapter_url_selector": "a",
			"href_selector": "href"
		},
		"images": {
			"selector": ".comic-contain amp-img",
			"src_attribute": "src"
		}
	}
}
